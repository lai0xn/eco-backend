model Organization {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  description String
  image       String
  bg_image    String
  ownerId     String   @db.ObjectId
  owner       User     @relation(fields: [ownerId], references: [id])
  eventsIds   String[]
  followers User[]  @relation("orgfollowers",fields: [followersIds], references: [id])
  followersIds String[] @db.ObjectId
  acheivments Acheivment[]
  events      Event[]
  createdAt     DateTime           @default(now())

}
